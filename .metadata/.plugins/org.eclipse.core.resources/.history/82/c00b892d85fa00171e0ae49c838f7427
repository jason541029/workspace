package edu.fju.exam;

public class Niner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
for (int n = 2; n<=5; n++){
	for (int i=6; i<=9; i++){
		System.out.print(n +"x"+1+"=");
		if((n*i)<10){
			System.out.print(" ");
		}
		System.out.println((n*1));
	}
	System.out.println();
}
	}

}
