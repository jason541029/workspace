package edu.fju.exam;

public class Bubble {
	public static void main(String[] args) {
		int n[] = {45, 81, 9, 72, 4, 6};
		
		for (int j=0; j<n.length-1;j++){
			for (int i=j+1;i<n.length; i++){
				if (n[i]>n[j]){
					int tmp = n[j];
					n[i] = n[j];
						n[i] = tmp;
					}
				}
	
		}
		for(int num : n)
			System.out.println(num+" ");
			}
		}

			
